  "main": "index.js",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "test": "yarn compile && hardhat test --no-compile",
    "test:rinkeby": "yarn compile && hardhat --config hardhat.test.rinkeby.ts test --no-compile",
    "coverage": "yarn build && hardhat coverage --temp artifacts",
    "dev": "nodemon",
    "dev:rinkeby": "nodemon --config nodemon.rinkeby.json",
    "node": "npx hardhat node",
    "node:rinkeby": "npx hardhat --config hardhat.test.rinkeby.ts node",
    "prettify": "prettier --write \"./**/*.{ts,sol}\"",
    "deploy": "hardhat deploy"
  },
  "repository": {
    "type": "git"
  },
